<script>
   function timeLineValidation(objIn1,objOut1,
      mon1,tue1,wed1,thu1,fri1,sat1,sun1,
      mon2,tue2,wed2,thu2,fri2,sat2,sun2,
      mon3,tue3,wed3,thu3,fri3,sat3,sun3,
      mon4,tue4,wed4,thu4,fri4,sat4,sun4,
      mon5,tue5,wed5,thu5,fri5,sat5,sun5, ctime, timeline) { 
           pause(500); 
      var objMon = document.getElementById(mon1);
      var objTue = document.getElementById(tue1);
      var objWed = document.getElementById(wed1);
      var objThu = document.getElementById(thu1);
      var objFri = document.getElementById(fri1);
      var objSat = document.getElementById(sat1);
      var objSun = document.getElementById(sun1);
      var inEntered =  document.getElementById(objIn1); 
      var outEntered = document.getElementById(objOut1);
      var TimeInEntered  = inEntered.value;
      var TimeOutEntered = outEntered.value;
      if (TimeInEntered == '' || TimeOutEntered == '')
         {
           objMon.disabled = true;
           objMon.checked = false;
           objTue.disabled = true;
           objTue.checked = false;
           objWed.disabled = true;
           objWed.checked = false;
           objThu.disabled = true;
           objThu.checked = false;
           objFri.disabled = true;
           objFri.checked = false;
           objSat.disabled = true;
           objSat.checked = false;
           objSun.disabled = true;
           objSun.checked = false;
        }
   else
        {
                var objMon2 = document.getElementById(mon2);
                var objMon3 = document.getElementById(mon3);
                var objMon4 = document.getElementById(mon4);
                var objMon5 = document.getElementById(mon5);
                if (objMon2.checked || objMon3.checked || objMon4.checked || objMon5.checked)
                   objMon.disabled = true;
                else 
                   objMon.disabled = false;
                var objTue2 = document.getElementById(tue2);
                var objTue3 = document.getElementById(tue3);
                var objTue4 = document.getElementById(tue4);
                var objTue5 = document.getElementById(tue5);
                if (objTue2.checked || objTue3.checked || objTue4.checked || objTue5.checked)
                   objTue.disabled = true;
                else 
                   objTue.disabled = false;   
                var objWed2 = document.getElementById(wed2);
                var objWed3 = document.getElementById(wed3);
                var objWed4 = document.getElementById(wed4);
                var objWed5 = document.getElementById(wed5);
                if (objWed2.checked || objWed3.checked || objWed4.checked || objWed5.checked)
                   objWed.disabled = true;
                else 
                   objWed.disabled = false;   
                var objThu2 = document.getElementById(thu2);
                var objThu3 = document.getElementById(thu3);
                var objThu4 = document.getElementById(thu4);
                var objThu5 = document.getElementById(thu5);
                if (objThu2.checked || objThu3.checked || objThu4.checked || objThu5.checked)
                   objThu.disabled = true;
                else 
                   objThu.disabled = false;   
                var objFri2 = document.getElementById(fri2);
                var objFri3 = document.getElementById(fri3);
                var objFri4 = document.getElementById(fri4);
                var objFri5 = document.getElementById(fri5);
                if (objFri2.checked || objFri3.checked || objFri4.checked || objFri5.checked)
                   objFri.disabled = true;
                else 
                   objFri.disabled = false;   
                var objSat2 = document.getElementById(sat2);
                var objSat3 = document.getElementById(sat3);
                var objSat4 = document.getElementById(sat4);
                var objSat5 = document.getElementById(sat5);
                if (objSat2.checked || objSat3.checked || objSat4.checked || objSat5.checked)
                   objSat.disabled = true;
                else 
                   objSat.disabled = false;   

               var objSun2 = document.getElementById(sun2);
               var objSun3 = document.getElementById(sun3);
               var objSun4 = document.getElementById(sun4);
               var objSun5 = document.getElementById(sun5);
               if (objSun2.checked || objSun3.checked || objSun4.checked || objSun5.checked)
                   objSun.disabled = true;
                else 
                   objSun.disabled = false;   
        }
     //   pause(500);
        if (ctime == 'timein')
            setTimeOut(TimeInEntered, timeline);
        else
          confirmTimeOut();    
          
   }
 </script>
