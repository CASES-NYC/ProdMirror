 <script>
     function setTimeOut(timeInValue, timeline)
     {
    //   pause(500);
       if (timeline == 1)
          {
             afSetTimeOutStart1(timeInValue);}
       else
          if (timeline == 2)
            {
              afSetTimeOutStart2(timeInValue);
            }
       else
          if (timeline == 3)
            {
               afSetTimeOutStart3(timeInValue);
            }
       else
          if (timeline == 4)
            {
              afSetTimeOutStart4(timeInValue);
            }
       else
          if (timeline == 5)
           {
             afSetTimeOutStart5(timeInValue);
           }
     } 
</script>

