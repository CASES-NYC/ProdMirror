    <hr style="width: 100%; color: black; height: 1px; background-color:black;" />
    <h4>FAMILY STRUCTURE</h4>
      <apex:inputField value="{!Health_and_Wellbeing_Assessment__c.Family_Structure__c}" styleClass="form-control"
