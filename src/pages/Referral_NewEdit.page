    <div class="row-fluid">
        <apex:outputText value="Name of Program"  styleclass="col-md-4" />
        <apex:outputText value="Date Started"  styleclass="col-md-4" />
        <apex:outputText value="Contact Information"  styleclass="col-md-4" />
