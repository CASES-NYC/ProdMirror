    <fields>
        <fullName>CRAFFT_Yes_in_Part_A__c</fullName>
        <externalId>false</externalId>
        <formula>or(ispickval(CRAFFT_A1_Drink_any_alcohol__c ,&quot;Yes&quot;), 
ispickval(CRAFFT_A2_Smoke_any_marijuana_or_hashish__c ,&quot;Yes&quot;), 
ispickval(CRAFFT_A3_Use_anything_else_to_get_high__c ,&quot;Yes&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Yes in Part A?</label>
