        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Score__c</fullName>
        <externalId>false</externalId>
        <formula>if(ispickval(CRAFFT_B1_In_CAR_driven_by_someone_high__c ,&quot;Yes&quot;),1,0) 
+ if(ispickval(CRAFFT_B2_Use_drugs_or_alcohol_to_RELAX__c ,&quot;Yes&quot;),1,0)
+ if(ispickval(CRAFFT_B3_Use_drugs_or_alcohol_ALONE__c ,&quot;Yes&quot;),1,0) 
+ if(ispickval(CRAFFT_B4_Ever_FORGET_what_you_did__c ,&quot;Yes&quot;),1,0) 
+ if(ispickval(CRAFFT_B5_FAMILY_FRIENDS_say_to_cut_down__c ,&quot;Yes&quot;),1,0) 
+ if(ispickval(CRAFFT_B6_In_TROUBLE_while_using_drugs__c ,&quot;Yes&quot;),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
