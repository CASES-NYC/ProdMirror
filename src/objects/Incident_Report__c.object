    <validationRules>
        <fullName>Check_Injury_Box_if_Completing_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>and(not(Participants_Injured__c), 
or(
not(isblank( Medical_Treatment_Provided__c )),
not(isblank( Participants_Projected_Return_to_Program__c )),
not(isblank( Activity_Limitations__c )),
not(isblank( Defect_at_Incident_Site__c )),
not(isblank( Measures_Taken_to_Remedy_Defect__c ))
)
)</errorConditionFormula>
        <errorDisplayField>Participants_Injured__c</errorDisplayField>
        <errorMessage>If filling out the injury report fields, check the Participants Injured checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Injury_Box_if_Harm_Caused</fullName>
        <active>true</active>
        <errorConditionFormula>and(
not(ispickval(Degree_of_Harm_Caused__c, &quot;No Harm&quot;)),
not(Participants_Injured__c)
)</errorConditionFormula>
        <errorDisplayField>Participants_Injured__c</errorDisplayField>
        <errorMessage>If you filled out the &quot;Degree of Harm Caused&quot; field, then check the &quot;Participants Injured&quot; box and fill out the Injury section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Property_Box_if_Completing_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>and(not(Property_Lost_Damaged_or_Stolen__c), 
or(
not(isblank(  Property_Losses__c  )),
not(isblank(  Police_Notified_of_Property_Loss__c  ))
)
)</errorConditionFormula>
        <errorDisplayField>Property_Lost_Damaged_or_Stolen__c</errorDisplayField>
        <errorMessage>If filling out the property loss report fields, check the Property Lost, Damaged or Stolen checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Incident_Name_less_than_35_characters</fullName>
        <active>true</active>
        <errorConditionFormula>len(Name) &gt; 35</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Please enter a brief incident description of 35 characters or less.</errorMessage>
    </validationRules>
