    <fields>
        <fullName>Calc_Class__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( Class__c , 
&quot;A&quot;, 900000, 
&quot;B&quot;, 800000,
&quot;C&quot;, 700000,
&quot;D&quot;, 600000,
&quot;E&quot;, 500000,
&quot;F&quot;, 400000,
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc_Class</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Calc_Level__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( Level__c , &quot;V&quot;, 1000000, &quot;M&quot;, 2000000,&quot;F&quot;,3000000,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc_Level</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Calc_Violent__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Violent__c , 10000,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc_Violent</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
