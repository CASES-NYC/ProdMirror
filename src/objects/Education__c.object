    <validationRules>
        <fullName>End_Date</fullName>
        <active>true</active>
        <description>End date is required to be filled if Reason For Leaving is any value other than &quot;Not Entered&quot;</description>
        <errorConditionFormula>AND(
 ISNULL( End_Date__c ), 
AND(
NOT(ISPICKVAL( Reason_for_Leaving__c, &quot;Not Entered&quot;)),
NOT(ISPICKVAL( Reason_for_Leaving__c, &quot;&quot;))
))</errorConditionFormula>
        <errorDisplayField>Reason_for_Leaving__c</errorDisplayField>
        <errorMessage>End Date cannot be left blank if Reason for Leaving is a value other than &quot;Not Entered&quot; or blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_date2</fullName>
        <active>true</active>
        <description>Reason For Leaving cannot be &quot;Not Entered&quot;  or blank if End date is filled</description>
        <errorConditionFormula>AND( NOT( ISNULL( End_Date__c )), 
OR(
ISPICKVAL( Reason_for_Leaving__c, &quot;Not Entered&quot;),
ISPICKVAL( Reason_for_Leaving__c, &quot;&quot;)
))</errorConditionFormula>
        <errorDisplayField>Reason_for_Leaving__c</errorDisplayField>
        <errorMessage>Reason For Leaving cannot be &quot;Not Entered&quot; or blank if End Date is entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GED_Program_GED_Diploma</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL(Institution_Type__c, &quot;GED Program&quot;), ISPICKVAL(Reason_for_Leaving__c, &quot;Graduated&quot;), NOT(ISPICKVAL(Diploma_Type__c, &quot;GED Diploma&quot;))), true, false)</errorConditionFormula>
        <errorDisplayField>Diploma_Type__c</errorDisplayField>
        <errorMessage>If a client has attended a GED Program and Graduated, the Diploma Type must be set to &quot;GED Diploma&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>High_School_High_School_Diploma</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISPICKVAL(Institution_Type__c, &quot;High School&quot;) 
&amp;&amp; ISPICKVAL(Reason_for_Leaving__c, &quot;Graduated&quot;) 
&amp;&amp; Not(ISPICKVAL(Diploma_Type__c, &quot;High School Diploma&quot;)) &amp;&amp; NOT(ISPICKVAL(Diploma_Type__c, &quot;IEP Diploma&quot;)), true, false)</errorConditionFormula>
        <errorDisplayField>Diploma_Type__c</errorDisplayField>
        <errorMessage>If a client has attended High School and Graduated, the Diploma Type must be set to &quot;High School Diploma&quot; or &quot;IEP Diploma&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Reason_for_Leaving</fullName>
        <active>true</active>
        <description>If Reason for Leaving is OTHER, then other text box has to be entered.</description>
        <errorConditionFormula>AND( ISPICKVAL(  Reason_for_Leaving__c  ,&quot;Other&quot;) , (  Reason_for_Leaving_Other__c   = &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Reason_for_Leaving_Other__c</errorDisplayField>
        <errorMessage>If Reason for Leaving is OTHER, Other text box has to be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_School_High_School_Diploma</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL(Institution_Type__c, &quot;PM School&quot;), ISPICKVAL(Reason_for_Leaving__c, &quot;Graduated&quot;), NOT(ISPICKVAL(Diploma_Type__c, &quot;High School Diploma&quot;))), true, false)</errorConditionFormula>
        <errorDisplayField>Diploma_Type__c</errorDisplayField>
        <errorMessage>If a client has attended PM School and Graduated, the Diploma Type must be set to &quot;High School Diploma&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_Leaving</fullName>
        <active>true</active>
        <description>You can only enter data in the &quot;Other&quot; text box if the &quot;Reason for Leaving&quot; is &quot;Other&quot;</description>
        <errorConditionFormula>AND ( NOT(ISPICKVAL(  Reason_for_Leaving__c  ,&quot;Other&quot;)),  (  Reason_for_Leaving_Other__c   &lt;&gt; &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Reason_for_Leaving_Other__c</errorDisplayField>
        <errorMessage>If Reason for Leaving is NOT OTHER, Other text box cannot entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_and_End_date_not_greater_than_toda</fullName>
        <active>true</active>
        <description>The School Start and End date cannot be greater than TODAY</description>
        <errorConditionFormula>IF( Start_Date__c &gt; TODAY(), true,  IF( End_Date__c &gt; TODAY(), true, false) )</errorConditionFormula>
        <errorMessage>The School Start and End date cannot be greater than today&apos;s date</errorMessage>
    </validationRules>
</CustomObject>
