    <validationRules>
        <fullName>Cred_Already_Comp_Before_1st_Semester</fullName>
        <active>true</active>
        <errorConditionFormula>and(
Credits_as_of_Date_RPT__c &gt;  First_Semester_Entered__c,
not(isblank(Credits_as_of_Date_RPT__c))
)</errorConditionFormula>
        <errorDisplayField>Semester__c</errorDisplayField>
        <errorMessage>You cannot set a &quot;Class Data Entry begins&quot; date that is later than the first semester entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cred_Already_Comp_Needs_Date_and_Value</fullName>
        <active>true</active>
        <errorConditionFormula>and( 

or( 
ispickval(Semester__c,&quot;&quot;), isblank(Credits_Already_Completed__c), 
ispickval(Year__c,&quot;&quot;) 
), 

or( 
not(ispickval(Semester__c,&quot;&quot;)), not(isblank(Credits_Already_Completed__c)), not(ispickval(Year__c,&quot;&quot;)) 
) 

)</errorConditionFormula>
        <errorDisplayField>Semester__c</errorDisplayField>
        <errorMessage>You must enter the semester, year, and number of credits which the client had completed at that time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dev_Ed_Math_1_Complete</fullName>
        <active>false</active>
        <errorConditionFormula>and( 

or( not(Dev_Ed_Math_1_Requirement_Complete__c), 
isblank( Dev_Ed_Math_1_Complete_Date__c ), 
ispickval( Dev_Ed_How_Math_1_Completed__c ,&quot;&quot;) 
), 

or( 
Dev_Ed_Math_1_Requirement_Complete__c, 
not(isblank(Dev_Ed_Math_1_Complete_Date__c)), 
not(ispickval(Dev_Ed_How_Math_1_Completed__c,&quot;&quot;)) 
) 

)</errorConditionFormula>
        <errorDisplayField>Dev_Ed_Math_1_Requirement_Complete__c</errorDisplayField>
        <errorMessage>If the Math 1 Requirement is complete, then enter a date and method, and put a check in the box.</errorMessage>
    </validationRules>
