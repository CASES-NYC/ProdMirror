        <fullName>DA_s_Plea_Offer_Max_Days__c</fullName>
        <externalId>false</externalId>
        <formula>if( ispickval( DA_s_Plea_Offer__c, &quot;Determinate Sentence&quot;),  DA_s_Plea_Offer_Min_Days__c , 

value(trim(Mid( DA_s_Plea_Offer_Sentence__c, 
find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c)+1, Max(find(&quot;ear&quot;,Mid(DA_s_Plea_Offer_Sentence__c, find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c)+1,len(DA_s_Plea_Offer_Sentence__c) - find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c))), 
find( &quot;ay&quot;, Mid(DA_s_Plea_Offer_Sentence__c, find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c)+1,len(DA_s_Plea_Offer_Sentence__c) - find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c))), 
find(&quot;onth&quot;, Mid(DA_s_Plea_Offer_Sentence__c, find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c)+1,len(DA_s_Plea_Offer_Sentence__c) - find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c))))-3 ))) 

* 

if(contains(RIGHT(DA_s_Plea_Offer_Sentence__c, 7), &quot;ay&quot;), 1, 
if(contains(RIGHT(DA_s_Plea_Offer_Sentence__c, 7), &quot;on&quot;), 30, 
if(contains(RIGHT(DA_s_Plea_Offer_Sentence__c, 7), &quot;ea&quot;), 365,0 
))) 

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DA&apos;s Plea Offer Max Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DA_s_Plea_Offer_Min_Days__c</fullName>
        <externalId>false</externalId>
        <formula>value(LEFT(LEFT( DA_s_Plea_Offer_Sentence__c, if(contains(DA_s_Plea_Offer_Sentence__c, &quot;-&quot;), find(&quot;-&quot;, DA_s_Plea_Offer_Sentence__c), len(DA_s_Plea_Offer_Sentence__c)) 
), Max(find(&quot;ear&quot;,LEFT(DA_s_Plea_Offer_Sentence__c, 7)), find( &quot;ay&quot;, LEFT(DA_s_Plea_Offer_Sentence__c, 7)), find(&quot;onth&quot;, LEFT(DA_s_Plea_Offer_Sentence__c, 7)))-3 )) 

* 

if(contains(LEFT(DA_s_Plea_Offer_Sentence__c, 7), &quot;ay&quot;), 1, 
if(contains(LEFT(DA_s_Plea_Offer_Sentence__c, 7), &quot;on&quot;), 30, 
if(contains(LEFT(DA_s_Plea_Offer_Sentence__c, 7), &quot;ea&quot;), 365,0 
)))</formula>
