        <formula>if( ispickval( Sentence_Type__c , &quot;Determinate Sentence&quot;),  Sentence_Min_Days__c , 

value(trim(Mid( Sentence__c, 
find(&quot;-&quot;, Sentence__c)+1, Max(find(&quot;ear&quot;,Mid(Sentence__c, find(&quot;-&quot;, Sentence__c)+1,len(Sentence__c) - find(&quot;-&quot;, Sentence__c))), 
find( &quot;ay&quot;, Mid(Sentence__c, find(&quot;-&quot;, Sentence__c)+1,len(Sentence__c) - find(&quot;-&quot;, Sentence__c))), 
find(&quot;onth&quot;, Mid(Sentence__c, find(&quot;-&quot;, Sentence__c)+1,len(Sentence__c) - find(&quot;-&quot;, Sentence__c))))-3 ))) 

* 

if(contains(RIGHT(Sentence__c, 7), &quot;ay&quot;), 1, 
if(contains(RIGHT(Sentence__c, 7), &quot;on&quot;), 30, 
if(contains(RIGHT(Sentence__c, 7), &quot;ea&quot;), 365,0 
))) 

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
