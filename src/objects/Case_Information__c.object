    <validationRules>
        <fullName>Arrest_Date_cant_be_in_the_future</fullName>
        <active>true</active>
        <description>Arrest Date can&apos;t be in the future</description>
        <errorConditionFormula>AND (Arrest_Date__c &gt; TODAY () )</errorConditionFormula>
        <errorDisplayField>Arrest_Date__c</errorDisplayField>
        <errorMessage>Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Arrest_Number_Validation</fullName>
        <active>false</active>
        <description>Format: 1 uppercase letter and 8 numbers C######## (no spaces).</description>
        <errorConditionFormula>AND(NOT(REGEX(Arrest_Number__c, &quot;([A-Z]\\d{8})&quot;)), NOT(ISBLANK(Arrest_Number__c)))</errorConditionFormula>
        <errorMessage>Arrest Numbers must be 1 UPPERCASE letter and 8 numbers: C######## (no spaces)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bond_Amount_MadeBail</fullName>
