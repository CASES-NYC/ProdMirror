        <errorConditionFormula>AND( AND ( (ISPICKVAL(   Detention_Status_at_CC_Arraignment__c  , &quot;In On Bail&quot;)),(ISPICKVAL(   Detention_Status_at_SC_Arraignment__c   , &quot;In On Bail&quot;))), ISNULL(  Bail_Set_Cash__c  ))</errorConditionFormula>
        <errorDisplayField>Bail_Set_Cash__c</errorDisplayField>
