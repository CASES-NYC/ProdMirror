    <fields>
        <fullName>Case_Court_Status__c</fullName>
        <description>Calculate what the actual stage is based on possible data entry combinations.

Can&apos;t include Bail Set - Bond because it defaults to 0 (not blank)</description>
        <externalId>false</externalId>
        <formula>IF (NOT(ISBLANK(Sentence__c)) || NOT(ISBLANK(Sentence_Disposition_Date__c)) || Youthful_Offender_Status__c = TRUE || Intensive_Supervision_Probation__c = TRUE, &quot;Final Disposition&quot;,
     IF (NOT(ISBLANK(Indictment__c)) || NOT(ISBLANK(Indictment_Date__c)) || NOT(ISBLANK(Top_Indictment_Charge__c)), &quot;Indicted&quot;,
          IF (NOT(ISBLANK(Criminal_Court_Arraignment_Date__c)) || NOT(ISBLANK(TEXT(Detention_Status_at_CC_Arraignment__c  ))) || NOT(ISBLANK(Top_Criminal_Court_Arraignment_Charge__c)) || NOT(ISBLANK(Bail_Set_Cash__c)) ||  RIGHT(TRIM(Name), 2) &lt;&gt; &quot;NA&quot; , &quot;Arraigned CC&quot;, 
              TEXT( Arraignment_Status_CC__c ))))</formula>
        <label>Case Court Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
