        <errorConditionFormula>AND( AND ( (ISPICKVAL(   Detention_Status_at_CC_Arraignment__c  , &quot;Made Bail&quot;)),(ISPICKVAL(   Detention_Status_at_SC_Arraignment__c   , &quot;Made Bail&quot;))), ISNULL( Bail_Set_Bond__c ))</errorConditionFormula>
        <errorDisplayField>Bail_Set_Bond__c</errorDisplayField>
        <errorMessage>Must enter a bond amount if Detention status at Criminal Court or Supreme Court Arraignment is Made Bail or In on Bail.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CC_Arraignment_Date_cant_be_in_future</fullName>
        <active>true</active>
