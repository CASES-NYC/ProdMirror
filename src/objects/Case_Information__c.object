    <fields>
        <fullName>Sentence_Min_Days__c</fullName>
        <externalId>false</externalId>
        <formula>value(LEFT(LEFT( Sentence__c , if(contains(Sentence__c, &quot;-&quot;), find(&quot;-&quot;, Sentence__c), len(Sentence__c)) 
), Max(find(&quot;ear&quot;,LEFT(Sentence__c, 7)), find( &quot;ay&quot;, LEFT(Sentence__c, 7)), find(&quot;onth&quot;, LEFT(Sentence__c, 7)))-3 )) 

* 

if(contains(LEFT(Sentence__c, 7), &quot;ay&quot;), 1, 
if(contains(LEFT(Sentence__c, 7), &quot;on&quot;), 30, 
if(contains(LEFT(Sentence__c, 7), &quot;ea&quot;), 365,0 
)))</formula>
