    </validationRules>
    <validationRules>
        <fullName>Cash_Amount_MadeBail</fullName>
        <active>false</active>
        <description>Enter a monetary Cash amount based on detention status at criminal court arraignment or supreme court arraignment (MADE BAIL)</description>
        <errorConditionFormula>AND( AND ( (ISPICKVAL(   Detention_Status_at_CC_Arraignment__c  , &quot;Made Bail&quot;)),(ISPICKVAL(   Detention_Status_at_SC_Arraignment__c   , &quot;Made Bail&quot;))), ISNULL(  Bail_Set_Cash__c  ))</errorConditionFormula>
