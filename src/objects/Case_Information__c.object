    <validationRules>
        <fullName>DA_Plea_Offer_Other</fullName>
        <active>false</active>
        <description>if other is selected in DA&apos;s Plea Offer, then Other Plea Offer Other is required</description>
        <errorConditionFormula>AND(ISPICKVAL(     DA_s_Plea_Offer__c  , &quot;Other&quot;),     If_Other_Plea_Offer__c   = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>If_Other_Plea_Offer__c</errorDisplayField>
        <errorMessage>Other Plea Offer is required if DA&apos;s Plea offer is OTHER.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Docket_No_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(REGEX( Name, &quot;(((20|19)([0-9]{2})[A-Z]{2}[0-9]{6}$)|([D|E][-][0-9]{4,9}[/][0-9]{2}$))|(CR-[0-9]{6}-[0-9]{2}[A-Z]{2}$)&quot;)),
Name &lt;&gt; &quot;Docket NA&quot;,
Name &lt;&gt; &quot;Pending&quot;,
DATEVALUE(CreatedDate) &gt;= DATE(2015,1,22)
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Docket format is YYYYXX000000  or CR-000000-00XX for Criminal and Supreme Court.

For Family Court, the format is D-123456789/YY (like D-33445/14). The petition number can be 4-9 digits.
Enter &quot;Docket NA&quot;  if there&apos;s no docket, and &quot;Pending&quot; for TBD.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Indictment_Date_cant_be_in_future</fullName>
        <active>true</active>
        <description>Indictment Date can&apos;t be in the future</description>
        <errorConditionFormula>AND (Indictment_Date__c &gt; TODAY () )</errorConditionFormula>
        <errorDisplayField>Indictment_Date__c</errorDisplayField>
        <errorMessage>Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Indictment_No_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(
     OR(LEN(  Indictment__c ) = 0,
     OR(
     AND(
 
  CONTAINS(&quot;S&quot;, LEFT( Indictment__c , 1)),
  CONTAINS(&quot;C&quot;,  MID( Indictment__c, 2, 1)),
  CONTAINS(&quot;I&quot;,  MID( Indictment__c, 3, 1)),
  CONTAINS(&quot;0123456789&quot;, MID( Indictment__c , 4,1)), 
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 5, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 6, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 7, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 8, 1)),
  MID( Indictment__c, 9, 1) = &quot;-&quot;,
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 10, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 11, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 12, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 13, 1))
  ),

  AND(
  CONTAINS(&quot;0123456789&quot;, LEFT( Indictment__c , 1)), 
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 2, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 3, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 4, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 5, 1)),
  MID( Indictment__c, 6, 1) = &quot;-&quot;,
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 7, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 8, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 9, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 10, 1)) 
)
 )
)
)</errorConditionFormula>
        <errorDisplayField>Indictment__c</errorDisplayField>
        <errorMessage>Valid Indictment # formats are : 99999-YYYY,   SCI99999-YYYY</errorMessage>
    </validationRules>
