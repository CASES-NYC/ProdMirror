    <fields>
        <fullName>Referral_Recommended__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Result__c ==&apos;Yes&apos;, &apos;Referral Recommended&apos;, &apos;&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
