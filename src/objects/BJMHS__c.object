        <externalId>false</externalId>
        <formula>if(or(ispickval(Referral_Recommended_by_Screener__c ,&quot;Yes&quot;),Referral_Recommended_by_Assessment__c = &quot;Yes&quot;),
&quot;Yes&quot;,
&quot;No&quot;)</formula>
