    </fields>
    <fields>
        <fullName>Referral_Recommended_by_Assessment__c</fullName>
        <externalId>false</externalId>
        <formula>if(or(
ispickval(Q7_Taking_Medication__c, &quot;Yes&quot;) , 
ispickval(Q8_Hospital__c, &quot;Yes&quot;) ,
(if( ispickval(Q1_Mind_Control__c, &quot;Yes&quot;), 1, 0) + if( ispickval(Q2_Mind_Reading__c, &quot;Yes&quot;) , 1, 0) + if( ispickval(Q3_weight_Gain__c, &quot;Yes&quot;) , 1, 0) + if(ispickval(Q4_More_Active__c, &quot;Yes&quot;) , 1, 0) + if( 
ispickval(Q5_Talking_Slowly__c, &quot;Yes&quot;) , 1, 0) + if(ispickval(Q6_Feel_Sinful__c, &quot;Yes&quot;) , 1, 0)) &gt;= 2
), &quot;Yes&quot;, &quot;No&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>&quot;Yes&quot; will be set if the user who administered the screening believes the client should be referred to mental health services, regardless of the outcome of the BJMHS assessment.</inlineHelpText>
        <label>Referral Recommended by Assessment</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
