    <validationRules>
        <fullName>Programs_Served_Selection</fullName>
        <active>true</active>
        <description>If &quot;Serves All Programs&quot; is checked, &quot;Programs Served&quot; may not have any values selected (vice versa too-implemented 10/22/13 MB)</description>
        <errorConditionFormula>or(

and(Serves_All_Programs__c , NOT(ISBLANK(Programs_Served__c))), 

and(not(Serves_All_Programs__c), isblank(Programs_Served__c))
)</errorConditionFormula>
        <errorDisplayField>Serves_All_Programs__c</errorDisplayField>
        <errorMessage>You must either check off &quot;Serves All Programs&quot;, or select specific programs from the list.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referrals_only_available_for_CareerEx</fullName>
        <active>true</active>
        <errorConditionFormula>and(
ispickval(Enroll_Referral_Only_New__c, &quot;Yes&quot;),
CategoryRPT__c != &apos;Career Exploration Training&apos;
)</errorConditionFormula>
        <errorDisplayField>Enroll_Referral_Only_New__c</errorDisplayField>
        <errorMessage>You can only specify Referrals for Career Exploration - Training classes.</errorMessage>
    </validationRules>
