    <validationRules>
        <fullName>Edit_End_Date</fullName>
        <active>true</active>
        <description>If Program Cycle record type = SRP, it is editable only if the user&apos;s profile has Edit Program Cycle End Date = True</description>
        <errorConditionFormula>IF(AND(RecordType.DeveloperName = &quot;SRP_Program_Cycle&quot;, $User.Edit_Program_Cycle_End_Date__c = False, ISCHANGED(End_Date__c)), True, false)</errorConditionFormula>
        <errorMessage>You do not have permission to edit the Program Cycle End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Enter_Mandatory_Sessions_Required</fullName>
        <active>false</active>
        <errorConditionFormula>and( or(RecordTypeId = &quot;012J00000008gv0&quot;, RecordTypeId = &quot;012J00000008gv5&quot;),  ispickval(Client__r.Client_Status__c, &quot;Active&quot;), not(ispickval(Cycle_Type__c, &quot;Voluntary&quot;)), isblank(Mandatory_Sessions_Required__c))
/*RecordTypes are for DCP and TCM*/</errorConditionFormula>
        <errorDisplayField>Mandatory_Sessions_Required__c</errorDisplayField>
        <errorMessage>You must enter the number of Mandatory Sessions Required, unless the Cycle Type is voluntary.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PEAK_Cycle_Exit_Reason_Other</fullName>
        <active>true</active>
        <description>If Program Cycle Record Type = PEAK Program Cycle,  Exit Reason = Other, and Exit Reason(Other) is Blank, display error message: : If Exit Reason is Other, please enter the reason in the Exit Reason(Other) text box.</description>
        <errorConditionFormula>IF((AND(RecordType.Name=&quot;PEAK Program Cycle&quot;,  TEXT( Exit_Reason__c ) = &quot;Other&quot;, ISBLANK(Exit_Reason_Other__c )      )), True, False)</errorConditionFormula>
        <errorDisplayField>Exit_Reason_Other__c</errorDisplayField>
        <errorMessage>If Exit Reason is Other, please enter the reason in the Exit Reason(Other) text box</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PEAK_Exit_reason_end_date</fullName>
        <active>true</active>
        <description>If Program Cycle Record Type = PEAK Program Cycle, and Exit Type is NOT blank, and End Date IS blank, display error message: You must enter an End Date in order to enter an Exit Type</description>
        <errorConditionFormula>IF(AND(RecordType.Name=&quot;PEAK Program Cycle&quot;,(ISBLANK( End_Date__c )), NOT(ISBLANK(TEXT( Exit_Type__c )))), True, False)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>You must enter an End Date in order to enter an Exit Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PEAK_not_transfer_end_date_exit_reason</fullName>
        <active>true</active>
        <description>If Program Cycle Record Type = PEAK Program Cycle, and Exit Type is NOT transfer, and End Date is NOT blank, and Exit Reason is blank, display error message: You must enter an Exit Reason</description>
        <errorConditionFormula>IF((AND(RecordType.Name=&quot;PEAK Program Cycle&quot;, NOT(
ISPICKVAL(Exit_Type__c, &quot;Transfer&quot;)), NOT(ISBLANK( End_Date__c )), ISBLANK(TEXT( Exit_Reason__c )))), True, False)</errorConditionFormula>
        <errorDisplayField>Exit_Reason__c</errorDisplayField>
        <errorMessage>If Exit Type is Successful Exit or Unsuccessful Exit, you must enter an Exit Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Cycle_Validation_Rule</fullName>
        <active>true</active>
        <description>If Program Cycle Record Type = PEAK Program Cycle, and End Date is NOT blank, and Exit Type is blank, display error message: You must enter an Exit Type in order to enter an End Date</description>
        <errorConditionFormula>IF(AND(RecordType.Name=&quot;PEAK Program Cycle&quot;, (ISBLANK(Text(Exit_Type__c))), NOT(ISBLANK(End_Date__c))), True, False)</errorConditionFormula>
        <errorDisplayField>Exit_Type__c</errorDisplayField>
        <errorMessage>You must enter an Exit Type in order to enter an End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unsuccessful_exit_validation</fullName>
        <active>false</active>
        <errorConditionFormula>and(ispickval(Exit_Type__c, &quot;Unsuccessful Exit&quot;),
or(
/*DTC*/
and( or(RecordType.DeveloperName = &quot;DTC Program Cycle&quot;, RecordType.DeveloperName = &quot;CCM Program Cycle&quot;),
Days_Sessions_Remaining__c = 0),

/*TRP*/
and( RecordType.DeveloperName = &quot;TRP Program Cycle&quot;, 
 TRP_Class_Status_RPT__c = &quot;Successful&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>Exit_Type__c</errorDisplayField>
        <errorMessage>You cannot mark a client as unsuccessfully exited if they have completed their mandate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zero_sessions_remaining_for_success_exit</fullName>
        <active>false</active>
        <errorConditionFormula>and(ispickval(Exit_Type__c, &quot;Successful Exit&quot;),
or(
/*DTC*/
and( or(RecordType.DeveloperName = &quot;DTC Program Cycle&quot;, RecordType.DeveloperName = &quot;CCM Program Cycle&quot;),
Days_Sessions_Remaining__c &gt; 0),

/*TRP*/
and( RecordType.DeveloperName = &quot;TRP Program Cycle&quot;, 
 TRP_Class_Status_RPT__c != &quot;Successful&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>Exit_Type__c</errorDisplayField>
        <errorMessage>You cannot mark a client as successfully exited if they have not completed their mandate.</errorMessage>
    </validationRules>
</CustomObject>
