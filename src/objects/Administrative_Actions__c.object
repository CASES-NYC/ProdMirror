    <validationRules>
        <fullName>Does_not_fit_criteria</fullName>
        <active>true</active>
        <description>Required if Participant does not Fit above criteria selected for Conditions for Reinstatement.&quot;</description>
        <errorConditionFormula>AND( ISPICKVAL( Conditions__c  , &quot;Participant does not fit above criteria&quot;),   isblank(Doesnt_Fit_Criteria_Comments__c ))</errorConditionFormula>
        <errorDisplayField>Doesnt_Fit_Criteria_Comments__c</errorDisplayField>
        <errorMessage>If the participant does not fit the criteria, you must add comments in the &quot;Doesn&apos;t Fit Criteria Comments&quot; box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Save_AA_before_approving</fullName>
        <active>true</active>
        <description>Must save before approving</description>
        <errorConditionFormula>and(ISNEW()=true, ispickval(Approved__c, &quot;Yes&quot;))</errorConditionFormula>
        <errorMessage>You must save the form before you can approve it. Please clear the check mark from the approval box and save again. Then edit, check approved, and save.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Status_on_Exit_Type</fullName>
        <active>true</active>
        <description>Task Status cannot be saved with a value of &quot;Done on Time&quot; or &quot;Incomplete&quot; if the Exit Type is not &quot;(S) Successful Exit/ Graduation&quot;.</description>
        <errorConditionFormula>IF(ISPICKVAL(Task_Status__c , &quot;Done On Time&quot;) ,  IF( ISPICKVAL( Exit_Type__c , &quot;(S) Successful Exit/ Graduation&quot;) , false, true) ,  IF( ISPICKVAL(Task_Status__c ,  &quot;Incomplete&quot;) , IF( ISPICKVAL( Exit_Type__c , &quot;(S) Successful Exit/ Graduation&quot;) , false, true), false) )</errorConditionFormula>
        <errorMessage>Task Status cannot be saved with a value of &quot;Done on Time&quot; or &quot;Incomplete&quot; if the Exit Type is not &quot;(S) Successful Exit/ Graduation&quot;.</errorMessage>
    </validationRules>
