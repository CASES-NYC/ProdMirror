    <validationRules>
        <fullName>Arrest_Number_1_Validation</fullName>
        <active>false</active>
        <description>Format: 1 uppercase letter and 8 numbers CNNNNNNNN (no spaces).</description>
        <errorConditionFormula>AND(NOT(REGEX(Arrest_Number__c, &quot;([A-Z]\\d{8})&quot;)))</errorConditionFormula>
        <errorMessage>Arrest Numbers must be 1 UPPERCASE letter and 8 numbers: CNNNNNNNN (no spaces)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Arrest_Number_2_Validation</fullName>
        <active>false</active>
        <description>Format: 1 uppercase letter and 8 numbers C######## (no spaces)</description>
        <errorConditionFormula>AND(NOT(REGEX(Arrest_Number2__c, &quot;([A-Z]\\d{8})&quot;)))</errorConditionFormula>
        <errorMessage>Format: 1 uppercase letter and 8 numbers C######## (no spaces).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Arrest_Number_3_Validation</fullName>
        <active>false</active>
        <description>Format: 1 uppercase letter and 8 numbers C######## (no spaces)</description>
        <errorConditionFormula>AND(NOT(REGEX(Arrest_Number3__c, &quot;([A-Z]\\d{8})&quot;)))</errorConditionFormula>
        <errorMessage>Format: 1 uppercase letter and 8 numbers C######## (no spaces)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Arrest_Number_4_Validation</fullName>
        <active>false</active>
        <description>Format: 1 uppercase letter and 8 numbers C######## (no spaces)</description>
        <errorConditionFormula>AND(NOT(REGEX(Arrest_Number4__c, &quot;([A-Z]\\d{8})&quot;)))</errorConditionFormula>
        <errorMessage>Format: 1 uppercase letter and 8 numbers C######## (no spaces)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BookAndCase</fullName>
        <active>true</active>
        <description>book and case number must be numeric</description>
        <errorConditionFormula>IF (not(isblank(BookCaseNumber__c)), 
    Not(isnumber(BookCaseNumber__c)),
NULL)</errorConditionFormula>
        <errorDisplayField>BookCaseNumber__c</errorDisplayField>
        <errorMessage>Book &amp; Case must be a 10 digit number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CJA_Score_Range</fullName>
        <active>true</active>
        <description>between -12 and 12</description>
        <errorConditionFormula>or(CJA_Score__c&lt;-12,CJA_Score__c&gt;12)</errorConditionFormula>
        <errorDisplayField>CJA_Score__c</errorDisplayField>
        <errorMessage>Valid CJA is between -12 and 12</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Indictment_No_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(
     OR(LEN(  Indictment__c ) = 0,
     OR(
     AND(
 
  CONTAINS(&quot;S&quot;, LEFT( Indictment__c , 1)),
  CONTAINS(&quot;C&quot;,  MID( Indictment__c, 2, 1)),
  CONTAINS(&quot;I&quot;,  MID( Indictment__c, 3, 1)),
  CONTAINS(&quot;0123456789&quot;, MID( Indictment__c , 4,1)), 
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 5, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 6, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 7, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 8, 1)),
  MID( Indictment__c, 9, 1) = &quot;-&quot;,
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 10, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 11, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 12, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 13, 1))
  ),

  AND(
  CONTAINS(&quot;0123456789&quot;, LEFT( Indictment__c , 1)), 
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 2, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 3, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 4, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 5, 1)),
  MID( Indictment__c, 6, 1) = &quot;-&quot;,
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 7, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 8, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 9, 1)),
  CONTAINS(&quot;0123456789&quot;,  MID( Indictment__c, 10, 1)) 
)
 )
)
)</errorConditionFormula>
        <errorDisplayField>Indictment__c</errorDisplayField>
        <errorMessage>Valid Indictment # formats are : 99999-YYYY,   SCI99999-YYYY</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NYSID_Format_Validation</fullName>
        <active>true</active>
        <description>Format: 8 numbers and 1 uppercase letter NNNNNNNNC (no spaces).</description>
        <errorConditionFormula>AND (len(NYSID__c) &gt; 0, NOT(AND( LEN( NYSID__c ) = 9, CONTAINS(&quot;0123456789&quot;, LEFT( NYSID__c , 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 2, 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 3, 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 4, 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 5, 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 6, 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 7, 1)), CONTAINS(&quot;0123456789&quot;, MID( NYSID__c , 8, 1)), CONTAINS(&quot;ABCDEFGHIJKLMNOPQRSTUVWXYZ&quot;, MID( NYSID__c , 9, 1)) )))</errorConditionFormula>
        <errorDisplayField>NYSID__c</errorDisplayField>
        <errorMessage>NYSID format must be 8 numbers and 1 UPPERCASE character. NNNNNNNNC (no spaces).</errorMessage>
    </validationRules>
