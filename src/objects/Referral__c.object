    <fields>
        <fullName>Age_at_Arrest_Calc__c</fullName>
        <description>Determine age at time of arrest</description>
        <externalId>false</externalId>
        <formula>IF(!IsNull(Arrest_Date__c), IF(!IsNull(DOB__c), FLOOR((Arrest_Date__c -  DOB__c)/ 365.2425), NULL),
FLOOR((TODAY() - DOB__c) / 365.2425))</formula>
