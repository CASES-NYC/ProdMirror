    <fields>
        <fullName>Attorney_Affiliation__c</fullName>
        <externalId>false</externalId>
        <formula>IF (ISPICKVAL(ContactId__r.Intake_Case__r.Affiliation__c, &quot;Legal Aid&quot;), &quot;LAS&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;Bronx Defenders&quot;), &quot;BXD&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;Brooklyn Defenders&quot;), &quot;BDS&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;New York County Defenders&quot;), &quot;NYD&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;Neighborhood Defenders of Harlem&quot;), &quot;NDS&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;Queens Law Associates&quot;), &quot;QLA&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;18-b&quot;), &quot;18B&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;Private&quot;), &quot;PVT&quot;,
IF (ISPICKVAL (ContactId__r.Intake_Case__r.Affiliation__c, &quot;Not Entered&quot;), &quot;&quot;,
TEXT(ContactId__r.Intake_Case__r.Affiliation__c))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Attorney Affiliation</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
