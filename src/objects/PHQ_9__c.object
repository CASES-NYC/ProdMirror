        <externalId>false</externalId>
        <formula>if (isNull (Score_Summary__c), &quot;&quot;,
if (Score_Summary__c &gt;= 20,
 &quot;Severe depression&quot;, if(Score_Summary__c &gt;= 15, 
&quot;Moderately severe depression&quot;, 
if(Score_Summary__c &gt;= 10, 
